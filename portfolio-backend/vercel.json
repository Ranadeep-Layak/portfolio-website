{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "CORS_ORIGIN": "@cors_origin",
    "PORT": "5000"
  }
}
